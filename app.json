{
  "expo": {
    "name": "epx-mobile",
    "slug": "epx-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "privacy":"public",
    "description":"Epx Mobile application to enrich education from public available source",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.ran.epx",
      "googleServicesFile":"./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "config":{
        "googleSignIn":{
          "certificateHash":""
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },    
    "githubUrl": "https://github.com/ranraj/epx-mobile"
  }
}
